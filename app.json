{
  "expo": {
    "name": "AarogyaMitraApp",
    "slug": "AarogyaMitraApp",
    "version": "1.0.0",
    "orientation": "portrait",
  "icon": "./assets/aarogyamitra-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
  "image": "./assets/aarogyamitra-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.venkat73.aarogyamitra",
      "infoPlist": {
        "NSSpeechRecognitionUsageDescription": "Allow $(PRODUCT_NAME) to use speech recognition.",
        "NSMicrophoneUsageDescription": "Allow $(PRODUCT_NAME) to use the microphone.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.venkat73.aarogyamitra",
      "adaptiveIcon": {
        "foregroundImage": "./assets/aarogyamitra-adaptive.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "61d24736-c397-4c19-8aed-98cfe6516ce8"
      },
      "geminiApiKey": "AIzaSyD6UM6QzkU10-uNZrp3KrbmL_xpRU0RtDo"
    },
    "owner": "venkat73",
    "plugins": [
      "expo-speech-recognition"
    ]
  }
}
